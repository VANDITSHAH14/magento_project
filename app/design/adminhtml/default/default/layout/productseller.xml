<?xml version="1.0"?>
<layout>
    <adminhtml_seller_index>
        <reference name="content">
            <block type="seller/adminhtml_seller" name="seller"></block>
        </reference>
    </adminhtml_seller_index>
    <adminhtml_seller_new>
        <update handle="adminhtml_seller_edit" />
    </adminhtml_seller_new>
    <adminhtml_seller_edit>
        <update handle="editor" />
        <reference name="content">
            <block type="seller/adminhtml_seller_edit" name="seller_edit"></block>
        </reference>
    </adminhtml_seller_edit>
    <adminhtml_sellerreport_index>
        <reference name='head'>
            <action method='addJs'>
                <script>jquery-3.6.0.min.js</script>
            </action>
            <action method='addJs'>
                <script>lib/jquery/noconflict.js</script>
            </action>
        </reference>
        <reference name="content">
            <block type="seller/adminhtml_sellerreport" name="sellerreport"></block>
        </reference>
    </adminhtml_sellerreport_index>
</layout>